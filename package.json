{
  "name": "@productdevbook/ts-i18n",
  "version": "0.1.0",
  "description": "Give your JavaScript the ability to speak many languages.",
  "homepage": "https://github.com/productdevbookcom/ts-i18n",
  "bugs": "https://github.com/productdevbookcom/ts-i18n/issues",
  "author": "Mehmet @productdevbook",
  "repository": {
    "type": "git",
    "url": "https://github.com/productdevbookcom/ts-i18n.git"
  },
  "funding": "https://github.com/sponsors/productdevbook",
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "internationalisation",
    "translation",
    "interpolation",
    "translate",
    "polyglot",
    "typescript i18n",
    "backend i18n"
  ],
  "packageManager": "pnpm@7.29.0",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --dts",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm build && bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@types/has": "^1.0.0",
    "@types/iterate-iterator": "^1.0.0",
    "@vitest/coverage-c8": "^0.29.3",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "iterate-iterator": "^1.0.2",
    "tsup": "^6.6.3",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  },
  "publishConfig": {
    "access": "public"
  }
}